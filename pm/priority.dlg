#ifndef OS2_INCLUDED
   #include <os2.h>
#endif

#include "priority.h"

DLGTEMPLATE NBKP_PRIORITY
BEGIN
    DIALOG "Priority", NBKP_PRIORITY, 42, 57, 262, 142,	FS_NOBYTEALIGN | NOT
		FS_DLGBORDER | WS_CLIPSIBLINGS | WS_SAVEBITS
    PRESPARAMS PP_USER,	"10.System Proportional"
    BEGIN
	CONTROL	"Downloading Engine:", -1, 18, 130, 131, 8, WC_STATIC, SS_TEXT |
					    DT_LEFT | DT_TOP | WS_GROUP	|
					    WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"Priority Class",      -1, 19, 48, 87, 72, WC_STATIC,
					    SS_GROUPBOX	| WS_GROUP | WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"Normal",	       PB_PRI_NORMAL, 27, 87, 73, 10, WC_BUTTON,
					    BS_AUTORADIOBUTTON | WS_TABSTOP |
					    WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"Idle Time",	       PB_PRI_IDLE, 27,	75, 73,	10, WC_BUTTON,
					    BS_AUTORADIOBUTTON | WS_TABSTOP |
					    WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"",		       CSLD_PRI_LEVEL, 146, 57,	55, 43,
					    WC_CIRCULARSLIDER, CSS_NONUMBER |
					    CSS_NOTEXT | CSS_NOTICKS | WS_GROUP	|
					    WS_TABSTOP | WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"Relative Priority",   GB_PRI_PRIORITY,	122, 49, 111, 72,
					    WC_STATIC, SS_GROUPBOX | WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"low",		       -1, 128,	77, 16,	8, WC_STATIC, SS_TEXT |
					    DT_LEFT | DT_TOP | WS_VISIBLE
