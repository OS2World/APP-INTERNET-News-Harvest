	CONTROL	"high",		       -1, 207,	77, 18,	8, WC_STATIC, SS_TEXT |
					    DT_LEFT | DT_TOP | WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"normal",	       -1, 163,	101, 28, 8, WC_STATIC, SS_TEXT |
					    DT_LEFT | DT_TOP | WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"Limit bandwidth to",  -1, 24, 30, 72, 8, WC_STATIC, SS_TEXT |
					    DT_LEFT | DT_TOP | WS_GROUP	|
					    WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"bytes / second (average)", -1,	164, 30, 83, 8,	WC_STATIC,
					    SS_TEXT | DT_LEFT |	DT_TOP |
					    WS_VISIBLE
		PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
	CONTROL	"PRI_BANDWIDTH",       SPB_PRI_BANDWIDTH, 89, 30, 57, 8,
					    WC_SPINBUTTON, SPBS_MASTER |
					    SPBS_JUSTRIGHT | WS_TABSTOP	|
					    WS_VISIBLE
		CTLDATA	24, 0, 256, 0, 0, 0, 0,	0, 0, 0, 0, 0
    END
